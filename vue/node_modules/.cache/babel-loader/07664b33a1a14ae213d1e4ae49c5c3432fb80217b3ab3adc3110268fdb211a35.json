{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PaseadoresService from \"@/services/PaseadoresService\";\nimport ReservasService from \"@/services/ReservasService\";\nimport NavCliente from \"@/components/Nav/NavCliente.vue\";\nimport store from \"@/store/index\";\nimport moment from 'moment';\nexport default {\n  name: \"PaseadorIndividual\",\n  components: {\n    NavCliente\n  },\n  data() {\n    return {\n      paseador: null\n    };\n  },\n  mounted() {\n    this.fetchPaseador();\n  },\n  methods: {\n    fetchPaseador() {\n      const paseadorId = this.$route.params.id;\n      PaseadoresService.get(paseadorId).then(response => {\n        this.paseador = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    reservarPaseador() {\n      const nuevaReserva = {\n        id_paseador: this.$route.params.id,\n        id_cliente: store.getters.getClientId,\n        fecha: moment(store.getters.getSelectedDate).format('YYYY/MM/DD'),\n        hora: store.getters.getSelectedTime\n      };\n      ReservasService.create(nuevaReserva).then(response => {\n        console.log(response.data);\n        this.$router.push(\"/reservas-cliente\");\n        store.commit('setNotification', 'success');\n        store.commit('setMessage', 'Paseo reservado!');\n      }).catch(error => {\n        console.log(\"Error al crear la reserva: \", error);\n        this.error = \"Error al crear la reserva.\" + error;\n      });\n    },\n    getImageUrl(filename) {\n      if (filename) {\n        const imagePath = filename.replace('vue', '').replace('public', '');\n        const baseUrl = window.location.origin;\n        return `${baseUrl}/${imagePath}`;\n      }\n    }\n  }\n};","map":{"version":3,"names":["PaseadoresService","ReservasService","NavCliente","store","moment","name","components","data","paseador","mounted","fetchPaseador","methods","paseadorId","$route","params","id","get","then","response","catch","error","console","log","reservarPaseador","nuevaReserva","id_paseador","id_cliente","getters","getClientId","fecha","getSelectedDate","format","hora","getSelectedTime","create","$router","push","commit","getImageUrl","filename","imagePath","replace","baseUrl","window","location","origin"],"sources":["/home/canseomatias/htdocs/canseo.site/vue/src/components/Cliente/PaseadorIndividual.vue"],"sourcesContent":["<template>\r\n  <NavCliente/>\r\n  <div class=\"general\">\r\n    <h1>Detalles de paseador</h1>\r\n    <div class=\"contenedor\" v-if=\"paseador\">\r\n      <img class=\"img-perfil\" :src=\"getImageUrl(paseador.foto_perfil || '/uploads/default-profile.png')\" :alt=\"paseador.nombre + ' ' + paseador.apellido\">\r\n      <p>{{ paseador.nombre }} {{ paseador.apellido }}</p>\r\n      <p><strong>Descripci√≥n:</strong> {{ paseador.descripcion }}</p>\r\n      <button class=\"reservar-paseador\" @click=\"reservarPaseador\">Reservar</button>\r\n    </div>\r\n    <div v-else>\r\n      <p>Cargando datos del paseador...</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport ReservasService from \"@/services/ReservasService\";\r\nimport NavCliente from \"@/components/Nav/NavCliente.vue\";\r\nimport store from \"@/store/index\";\r\nimport moment from 'moment';\r\n\r\nexport default {\r\n  name: \"PaseadorIndividual\",\r\n  components: {\r\n    NavCliente,\r\n  },\r\n  data() {\r\n    return {\r\n      paseador: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchPaseador();\r\n  },\r\n  methods: {\r\n    fetchPaseador() {\r\n      const paseadorId = this.$route.params.id;\r\n      PaseadoresService.get(paseadorId)\r\n        .then((response) => {\r\n          this.paseador = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    reservarPaseador() {\r\n\r\n      const nuevaReserva = {\r\n        id_paseador: this.$route.params.id,\r\n        id_cliente: store.getters.getClientId,\r\n        fecha: moment(store.getters.getSelectedDate).format('YYYY/MM/DD'),\r\n        hora: store.getters.getSelectedTime,\r\n      };\r\n\r\n      ReservasService.create(nuevaReserva)\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.$router.push(\"/reservas-cliente\");\r\n          store.commit('setNotification', 'success');\r\n          store.commit('setMessage', 'Paseo reservado!');\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al crear la reserva: \", error);\r\n          this.error = \"Error al crear la reserva.\" + error;\r\n        });\r\n\r\n    },\r\n    getImageUrl(filename) {\r\n      if (filename) {\r\n        const imagePath = filename.replace('vue', '').replace('public', '');\r\n        const baseUrl = window.location.origin;\r\n        return `${baseUrl}/${imagePath}`;\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.general{\r\n  min-height: 60vh;  \r\n  margin: 0 auto;\r\n  padding-left: 10px;\r\n  padding-right: 10px;\r\n  margin-top: 15px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  padding-bottom: 40px;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.img-perfil{\r\n  width: 150px;\r\n  border-radius: 100px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.reservar-paseador{\r\n  border-radius: 5px;\r\n  border: 1px solid #8DBD47;\r\n  background-color: #8DBD47;\r\n  font-size: 14px;\r\n  padding: 3px 15px;\r\n  width: 100%;\r\n  height: 40px;\r\n  margin-top: 45px;\r\n  margin-bottom: 45px;\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n}\r\n\r\n.contenedor{\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  width: 80vw;\r\n}\r\n\r\n</style>\r\n"],"mappings":";AAiBA,OAAOA,iBAAgB,MAAO,8BAA8B;AAC5D,OAAOC,eAAc,MAAO,4BAA4B;AACxD,OAAOC,UAAS,MAAO,iCAAiC;AACxD,OAAOC,KAAI,MAAO,eAAe;AACjC,OAAOC,MAAK,MAAO,QAA