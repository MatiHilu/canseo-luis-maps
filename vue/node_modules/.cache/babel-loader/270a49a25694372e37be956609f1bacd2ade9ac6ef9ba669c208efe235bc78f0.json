{"ast":null,"code":"import PaseadoresService from \"@/services/PaseadoresService\";\nimport store from \"@/store\";\nimport NavPaseador from \"@/components/Nav/NavPaseador.vue\";\nexport default {\n  name: \"PerfilPaseador\",\n  components: {\n    NavPaseador\n  },\n  data() {\n    return {\n      paseador: {}\n    };\n  },\n  mounted() {\n    const paseadorId = store.getters.getClientId;\n    this.fetchPaseador(paseadorId);\n  },\n  computed: {\n    notification() {\n      return store.getters.getNotification;\n    },\n    message() {\n      return store.getters.getMessage;\n    },\n    notificationClass() {\n      return {\n        error: this.notification === \"error\",\n        success: this.notification === \"success\"\n      };\n    }\n  },\n  methods: {\n    fetchPaseador(paseadorId) {\n      PaseadoresService.get(paseadorId).then(response => {\n        this.paseador = response.data;\n      }).catch(error => {\n        console.log(\"Error al obtener el perfil del paseador: \", error);\n      });\n    },\n    getImageUrl(filename) {\n      if (filename) {\n        const imagePath = filename.replace('vue', '').replace('public', '');\n        const baseUrl = window.location.origin;\n        return `${baseUrl}/${imagePath}`;\n      }\n    }\n  }\n};","map":{"version":3,"names":["PaseadoresService","store","NavPaseador","name","components","data","paseador","mounted","paseadorId","getters","getClientId","fetchPaseador","computed","notification","getNotification","message","getMessage","notificationClass","error","success","methods","get","then","response","catch","console","log","getImageUrl","filename","imagePath","replace","baseUrl","window","location","origin"],"sources":["/home/canseomatias/htdocs/canseo.site/vue/src/components/Paseador/PerfilPaseador.vue"],"sourcesContent":["<template>\r\n  <NavPaseador/>\r\n  <div class=\"general\">\r\n    <div v-if=\"notification\" class=\"notification\" :class=\"notificationClass\">\r\n      {{ message }}\r\n    </div>\r\n    <h1>Mi perfil</h1>\r\n    <div>\r\n      <div class=\"img-usuario\">\r\n      <p>Nombre: {{ paseador.nombre }} {{ paseador.apellido }}</p>\r\n      <img class=\"img-perfil\" :src=\"getImageUrl(paseador.foto_perfil || '/uploads/default-profile.png')\" alt=\"Foto de perfil\">\r\n    </div>\r\n      <p>Email: {{ paseador.email }}</p>\r\n      <p>Teléfono: {{ paseador.telefono }}</p>\r\n      <p>Dirección: {{ paseador.direccion }}, {{ paseador.nombre_barrio }}</p>\r\n      <p>Descripción: {{ paseador.descripcion }}</p>    \r\n      <p>Días de trabajo: <span v-if=\"paseador.dias_disponibles == 0\">No seleccionado<br/>(Para poder aparecer en las busquedas tenés que tener al menos 1 día disponible para trabajar)</span>\r\n  <span v-else v-for=\"(dia, index) in paseador.dias_disponibles\" :key=\"index\">\r\n    {{ dia === 'domingo' ? 'Domingo' : (dia === 'lunes' ? 'Lunes' : (dia === 'martes' ? 'Martes' : (dia === 'miercoles' ? 'Miércoles' : (dia === 'jueves' ? 'Jueves' : (dia === 'viernes' ? 'Viernes' : (dia === 'sabado' ? 'Sábado' : dia)))))) }}\r\n    <span v-if=\"index !== paseador.dias_disponibles.length - 1\">| </span>\r\n  </span>\r\n</p>\r\n   \r\n    </div>\r\n  \r\n    <router-link  :to=\"'/editar-paseador/'\">\r\n      <button>Editar perfil</button>\r\n    </router-link>\r\n    <router-link class=\"cambiar-contrasena\"  :to=\"'/cambiar-contrasena/paseador'\">\r\n        <button>Cambiar contraseña</button>\r\n    </router-link>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport store from \"@/store\";\r\nimport NavPaseador from \"@/components/Nav/NavPaseador.vue\";\r\n\r\nexport default {\r\n  name: \"PerfilPaseador\",\r\n  components: {\r\n    NavPaseador,\r\n  },\r\n  data() {\r\n    return {\r\n      paseador: {},     \r\n    };\r\n  },\r\n  mounted() {\r\n    const paseadorId = store.getters.getClientId;\r\n    this.fetchPaseador(paseadorId);\r\n  },\r\n  computed: {\r\n    notification() {\r\n      return store.getters.getNotification;\r\n    },\r\n    message() {\r\n      return store.getters.getMessage;\r\n    },\r\n    notificationClass() {\r\n      return {\r\n        error: this.notification === \"error\",\r\n        success: this.notification === \"success\"\r\n      };\r\n    }\r\n  },\r\n  methods: {\r\n    fetchPaseador(paseadorId) {\r\n      PaseadoresService.get(paseadorId)\r\n        .then((response) => {\r\n          this.paseador = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al obtener el perfil del paseador: \", error);\r\n        });\r\n    },\r\n    getImageUrl(filename) {\r\n  if (filename) {\r\n    \r\n    const imagePath = filename.replace('vue', '').replace('public', '');\r\n    const baseUrl = window.location.origin;\r\n    \r\n    return `${baseUrl}/${imagePath}`;\r\n  }\r\n}\r\n\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.general{\r\n  min-height: 60vh;  \r\n  margin: 0 auto;\r\n  padding-left: 10px;\r\n  padding-right: 10px;\r\n  margin-top: 15px;\r\n  padding-bottom: 40px;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  margin-bottom: 15px;\r\n}\r\n  \r\n  .form-group {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  input,\r\n  textarea,\r\n  select {\r\n    width: 100%;\r\n    padding: 5px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  button {\r\n  border-radius: 5px;\r\n  border: 1px solid #8DBD47;\r\n  background-color: #8DBD47;\r\n  font-size: 16px;\r\n  padding: 4px 8px;\r\n  cursor: pointer;\r\n  align-self: center;\r\n  margin-bottom: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n  .img-usuario{\r\n  display: flex;\r\n    flex-direction: row;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    margin-bottom: -45px;\r\n}\r\n\r\n.img-perfil{\r\n  width: 120px;\r\n  border-radius: 100px;\r\n}\r\n\r\n.notification {\r\n  margin-bottom: 10px;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n\r\n}\r\n\r\n.error {\r\n  background-color: #f2dede;\r\n  color: #a94442;\r\n}\r\n\r\n.success {\r\n  background-color: #dff0d8;\r\n  color: #3c763d;\r\n}\r\n\r\n.cambiar-contrasena{\r\n  margin-left: 15px;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,OAAOA,iBAAgB,MAAO,8BAA8B;AAC5D,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,WAAU,MAAO,kCAAkC;AAE1D,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CAAC;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,UAAS,GAAIP,KAAK,CAACQ,OAAO,CAACC,WAAW;IAC5C,IAAI,CAACC,aAAa,CAACH,UAAU,CAAC;EAChC,CAAC;EACDI,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAOZ,KAAK,CAACQ,OAAO,CAACK,eAAe;IACtC,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,OAAOd,KAAK,CAACQ,OAAO,CAACO,UAAU;IACjC,CAAC;IACDC,iBAAiBA,CAAA,EAAG;MAClB,OAAO;QACLC,KAAK,EAAE,IAAI,CAACL,YAAW,KAAM,OAAO;QACpCM,OAAO,EAAE,IAAI,CAACN,YAAW,KAAM;MACjC,CAAC;IACH;EACF,CAAC;EACDO,OAAO,EAAE;IACPT,aAAaA,CAACH,UAAU,EAAE;MACxBR,iBAAiB,CAACqB,GAAG,CAACb,UAAU,EAC7Bc,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACjB,QAAO,GAAIiB,QAAQ,CAAClB,IAAI;MAC/B,CAAC,EACAmB,KAAK,CAAEN,KAAK,IAAK;QAChBO,OAAO,CAACC,GAAG,CAAC,2CAA2C,EAAER,KAAK,CAAC;MACjE,CAAC,CAAC;IACN,CAAC;IACDS,WAAWA,CAACC,QAAQ,EAAE;MACxB,IAAIA,QAAQ,EAAE;QAEZ,MAAMC,SAAQ,GAAID,QAAQ,CAACE,OAAO,CAAC,KAAK,EAAE,EAAE,CAAC,CAACA,OAAO,CAAC,QAAQ,EAAE,EAAE,CAAC;QACnE,MAAMC,OAAM,GAAIC,MAAM,CAACC,QAAQ,CAACC,MAAM;QAEtC,OAAQ,GAAEH,OAAQ,IAAGF,SAAU,EAAC;MAClC;IACF;EAGE;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}